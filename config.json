{
  "useBackup": true,
  "geojsonFile": "Food_Hygiene_Ratings.geojson",
  "backupGeojsonFile": "Backup_Food_Hygiene_Ratings.geojson",
  "apiURL": "https://your-api-url.com/api",
  "defaultLocation": [51.4545, -2.5879],
  "enableGeolocation": true,
  "mapZoomLevel": 12,
  "mapTileLayer": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png",

  "chatbotEnabled": true,
  "chatbotGreeting": "Hi there, how can I help you?",
  "chatbotIcon": "https://cdn-icons-png.flaticon.com/128/10817/10817417.png",
  "chatbotAvatar": "https://cdn-icons-png.flaticon.com/512/4712/4712027.png",
  "enableChatbotMinimize": true,

  "chatbotUI": {
    "headerText": "HelpBot",
    "minimizeText": "Minimise",
    "consumerOptions": [
      "Report a Food Safety Issue",
      "Report a Product Labelling Issue",
      "Report an Unregistered Food Business",
      "Report a Problem with a Food Hygiene Rating",
      "Whistleblowing and Reporting a Food Crime"
    ],
    "businessOptions": [
      "Report a Food Safety Incident",
      "Whistleblowing and Reporting a Food Crime",
      "Report a Problem with a Food Hygiene Rating"
    ],
    "buttonStyle": {
      "backgroundColor": "#2F7D68",
      "color": "white",
      "borderRadius": "5px",
      "width": "100%",
      "padding": "10px",
      "fontSize": "16px"
    }
  },

  "defaultFilters": {
    "rating": null,
    "location": null,
    "businessName": null
  }
}
